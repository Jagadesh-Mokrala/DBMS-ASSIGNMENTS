insert into exam values(106,60,75,60,0,0);

insert into exam values(107,70,75,80,0,0);

insert into exam values(108,85,65,75,0,0);

insert into exam values(109,50,60,80,0,0);

insert into exam values(110,69,85,70,0,0);

update exam set total=s1+s2+s3;

update exam set avg=total/3;

select *from exam;

